equalList(l1, l2) {
    var e1 = isEmpty(l1);
    var e2 = isEmpty(l2);
    if(e1 || e2) {
        return e1 && e2;
    }
    return l1.hd == l2.hd && equalList(l1.tl, l2.tl);
}

